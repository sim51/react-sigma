{
  "name": "@react-sigma/website",
  "version": "4.0.3",
  "private": true,
  "scripts": {
    "build": "TYPEDOC_WATCH=false docusaurus build",
    "clean": "docusaurus clear && rimraf docs/api/*",
    "deploy": "docusaurus deploy",
    "docusaurus": "docusaurus",
    "serve": "TYPEDOC_WATCH=false docusaurus serve --port=4000",
    "start": "TYPEDOC_WATCH=true docusaurus start --port=4000",
    "swizzle": "docusaurus swizzle",
    "typecheck": "tsc",
    "write-heading-ids": "docusaurus write-heading-ids",
    "write-translations": "docusaurus write-translations"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@docusaurus/core": "^3.2.0",
    "@docusaurus/module-type-aliases": "^3.2.0",
    "@docusaurus/preset-classic": "^3.2.0",
    "@react-sigma/core": "^4.0.3",
    "@react-sigma/layout-circlepack": "^4.0.3",
    "@react-sigma/layout-circular": "^4.0.3",
    "@react-sigma/layout-core": "^4.0.3",
    "@react-sigma/layout-force": "^4.0.3",
    "@react-sigma/layout-forceatlas2": "^4.0.3",
    "@react-sigma/layout-noverlap": "^4.0.3",
    "@react-sigma/layout-random": "^4.0.3",
    "@docusaurus/tsconfig": "3.0.0",
    "clsx": "^2.1.0",
    "docusaurus-plugin-matomo": "^0.0.8",
    "docusaurus-plugin-sass": "^0.2.2",
    "docusaurus-plugin-typedoc": "^0.22.0",
    "jsx-to-string": "^1.4.0",
    "node-sass": "^9.0.0",
    "prism-react-renderer": "^2.3.1",
    "raw-loader": "^4.0.2",
    "react-live": "^4.1.6",
    "typedoc-plugin-markdown": "^3.12.1"
  }
}
